{
  "name": "nuxo_front_update",
  "version": "0.0.3",
  "main": "app/public/main.js",
  "description": "nuxo_front_update project",
  "author": "Cesar Casadiego",
  "scripts": {
    "dev": "concurrently \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:4000 && electron .\"",
    "start": "webpack serve --open",
    "build": "webpack --mode production",
    "publish": "yarn electron-builder --win -p always"
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "babel-loader": "^8.2.2",
    "electron": "^14.0.1",
    "electron-builder": "^22.11.7",
    "electron-notarize": "^1.1.1",
    "html-webpack-plugin": "^5.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "webpack": "^5.36.1",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "concurrently": "^6.4.0",
    "cross-env": "^7.0.3",
    "electron-log": "^4.4.1",
    "electron-updater": "^4.3.9",
    "wait-on": "^6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CC25MC/nuxo_front_update"
  },
  "bugs": {
    "url": "https://github.com/CC25MC/nuxo_front_update/issues"
  },
  "homepage": "git+https://github.com/CC25MC/nuxo_front_update#readme",
  "build": {
    "appId": "com.github.CC25MC.nuxofrontupdate",
    "afterSign": "./build/notarize.js",
    "mac": {
      "category": "your.app.category.type"
    },
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false
    },
    "publish": [
      {
        "provider": "github",
        "owner": "CC25MC",
        "repo": "nuxo_front_update"
      }
    ],
    "linux": {
      "category": "your.app.category.type",
      "target": "AppImage"
    }
  }
}
