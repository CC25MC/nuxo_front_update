{
  "name": "nuxo_front_update",
  "version": "0.0.13",
  "main": "public/main.js",
  "description": "nuxo_front_update project",
  "author": "Cesar Casadiego",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron:serve": "concurrently -k \"cross-env BROWSER=none yarn start\" \"yarn electron:start\"",
    "electron:build": "yarn build && electron-builder -p always -c.extraMetadata.main=build/main.js",
    "electron:start": "wait-on tcp:3000 && electron .",
    "publish": "yarn build && electron-builder -c.extraMetadata.main=build/main.js -p always"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "electron": "^11.2.1",
    "electron-builder": "^22.9.1",
    "prettier": "^2.2.1",
    "wait-on": "^5.2.1"
  },
  "dependencies": {
    "@electron/remote": "^1.0.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "bootstrap": "^5.0.0-beta1",
    "electron-is-dev": "^1.2.0",
    "electron-updater": "^4.3.9",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.1",
    "web-vitals": "^0.2.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CC25MC/nuxo_front_update"
  },
  "bugs": {
    "url": "https://github.com/CC25MC/nuxo_front_update/issues"
  },
  "homepage": "./",
  "build": {
    "appId": "com.github.CC25MC.nuxofrontupdate",
    "mac": {
      "category": "your.app.category.type"
    },
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false
    },
    "publish": [
      {
        "provider": "github",
        "owner": "CC25MC",
        "repo": "nuxo_front_update"
      }
    ],
    "linux": {
      "category": "your.app.category.type",
      "target": "AppImage"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
